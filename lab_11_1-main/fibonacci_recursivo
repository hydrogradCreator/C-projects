#include<stdio.h>
#include <stdlib.h>



long int fib(int n) {
    if(n == 1 || n == 2) {
        return 1;
    }
    
    else{
        return fib(n - 1) + fib(n - 2); 
    }
}

int main(){
    long int indice;

    scanf("%ld",&indice);
    long int fibb = fib(indice);

    printf("%ld", fibb);

    return 0;
}
